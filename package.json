{
  "name": "vue-body-map",
  "description": "A clickable body map built in Vue",
  "version": "0.1.1",
  "private": false,
  "author": {
    "name": "Nick Davies"
  },
  "repository": "https://github.com/nickdavies791/vue-body-map",
  "keywords": [
    "vue",
    "clickable",
